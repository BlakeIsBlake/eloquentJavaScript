function fizzBuzz(){
    for(var i = 1; i <= 100; i++){
        var outputString = "";
        if(i % 3 == 0){
         outputString += "Fizz";   
        }
        if(i % 5 == 0){
         outputString += "Buzz";   
        }
        if(outputString == ""){
         outputString = i;   
        }
        console.log(outputString);
    }    
}

fizzBuzz();
